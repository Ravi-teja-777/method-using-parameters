import java.util.*;
import java.io.*;
import java.lang.*;

public class Github{
    
    static void method(String fname){
        System.out.println("Your name is : "+fname);
    }
    
    public static void main(String args[]){
        Scanner x = new Scanner(System.in);
        System.out.print("Enter your name : ");
        String a = x.nextLine();
        method(a);
    }
}
